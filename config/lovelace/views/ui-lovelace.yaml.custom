                    - type: state-label
                      entity: device_tracker.michel_oneplus6t
                      prefix: 'Michel -- '
                      style: {color: white, left: 30%, top: 90%}
                    - type: state-icon
                      entity: sensor.battery_michel
                      tap_action: more_info
                      style: {color: white, left: 73%, top: 90%}
                    - type: state-label
                      entity: sensor.battery_michel
                      tap_action: more_info
                      style: {color: white, left: 87%, top: 91%}
